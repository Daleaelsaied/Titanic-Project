      "cell_type": "code",
      "source": [
        "# Average fare\n",
        "avg_fare = df['Fare'].mean()\n",
        "# New column: HighFare (1 if fare > avg, else 0)\n",
        "df['HighFare'] = np.where(df['Fare'] > avg_fare, 1, 0)\n",
        "# Survival rate\n",
        "df.groupby('HighFare')['Survived'].mean() * 100"

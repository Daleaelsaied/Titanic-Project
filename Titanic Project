        "# Fill with mode (most frequent)\n",
        "df['Embarked'].fillna(df['Embarked'].mode()[0], inplace=True)\n",
        "# Drop Cabin (too many missing)\n",
        "df.drop(columns=['Cabin'], inplace=True)"
